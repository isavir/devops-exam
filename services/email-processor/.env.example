# AWS Configuration
AWS_REGION=us-east-1
AWS_ACCESS_KEY_ID=your_access_key_here
AWS_SECRET_ACCESS_KEY=your_secret_key_here

# SQS Configuration - Retrieved from SSM Parameter Store
SQS_QUEUE_URL_PARAMETER=/email-service/sqs-queue-url

# Note: SQS_QUEUE_URL is now retrieved from SSM Parameter Store
# The parameter /email-service/sqs-queue-url contains the actual queue URL

# S3 Configuration
S3_BUCKET_NAME=email-storage-bucket

# Processing Configuration
POLL_INTERVAL_SECONDS=30
MAX_MESSAGES_PER_POLL=10
VISIBILITY_TIMEOUT_SECONDS=300